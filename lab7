package ua.edu.uabs.sierik.lab1;
/**
	 * @author Sierik
	 * This class used to create a new element and output the result
	 */
import ua.edu.uabs.sierik.lab1_1.Calculator;

public class First {
	/**
		 * @param args - main param
		 */
	public static void main(String[] args) {
			if (args.length == 0) {
		//System.out.println ("Hello World!");
		Calculator elem = new Calculator();
		double rez = elem.getResult(10, 7);
		System.out.println("Result= "+ rez);
	}
		else {
			for (int i = 0; i < args.length; i++) {
				System.out.println ("Element # " + i + ": " + args[i].toString());
			}
		}
	}
}
